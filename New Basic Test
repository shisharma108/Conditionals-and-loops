n=int(input())
i=1
p=1
while i<=n:
    j=1
    if i==1:
        print(p,end="")
        p=p+1
    elif i==2:
        print(p,end='')
        print(p+1,end="")
        p=p+2
    else:
        while j<=2**(i-1):
            if p==10:
                p=1
                print(p,end="")
                p+=1
            else:
                print(p,end="")
                p=p+1
            j=j+1
    print()
    i=i+1
            
            
            
    
